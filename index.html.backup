        // Funktion zum Mischen eines Arrays
        function shuffleArray(array) {
            for (let i = array.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [array[i], array[j]] = [array[j], array[i]];
            }
            return array;
        }

        // Funktion zum Generieren einer zuf√§lligen Variante einer Basis-Frage.
        // Ziel: h√§ufig den Basistyp beibehalten, aber sinnvolle Varianten (insb. matching/sort)
        // erzeugen und dabei korrekte Antworten konsistent halten.
        // Hilfsfunktion zum Mischen und Ausw√§hlen von n Elementen aus einem Array
        function getRandomElements(array, n) {
            const shuffled = [...array].sort(() => 0.5 - Math.random());
            return shuffled.slice(0, n);
        }

