        function initializeQuestionPool() {
            // OPTIMIERT: Nutze deduplizierte questionDatabase
            // questionPool wird nicht mehr ben√∂tigt, aber f√ºr Kompatibilit√§t behalten
            questionPool = {
                uebung: getQuestionsByLevel('uebung'),
